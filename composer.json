{
    "name": "cachethq/segment",
    "description": "Segment.com wrapper written for Laravel.",
    "keywords": ["laravel", "framework", "segment", "segment.com", "segment.io", "api", "Laravel Segment", "CachetHQ", "cachet"],
    "license": "MIT",
    "authors": [
        {
            "name": "James Brooks",
            "email": "james@cachethq.io"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "illuminate/config": "~5.0",
        "illuminate/support": "~5.0",
        "graham-campbell/manager": "~2.0",
        "segmentio/analytics-php": "~1.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "CachetHQ\\Segment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CachetHQ\\Tests\\Segment\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
